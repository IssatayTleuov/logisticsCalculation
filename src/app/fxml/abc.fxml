<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.AbcController">
   <children>
      <TableView fx:id="abcTable" layoutX="112.0" layoutY="100.0" prefHeight="200.0" prefWidth="377.0">
        <columns>
          <TableColumn fx:id="colName" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="colProfit" prefWidth="75.0" text="Profit" />
            <TableColumn fx:id="colShare" prefWidth="75.0" text="Share" />
            <TableColumn fx:id="colTotalShare" prefWidth="75.0" text="Total share" />
            <TableColumn fx:id="colGroup" prefWidth="75.0" text="Group" />
        </columns>
      </TableView>
      <Button fx:id="btnDoCalAbc" layoutX="411.0" layoutY="325.0" mnemonicParsing="false" onMouseClicked="#onMouseClickShowResult" text="Do calculation" visible="false" />
       <Button fx:id="btnPopulateTableAbc" layoutX="112.0" layoutY="325.0" mnemonicParsing="false" onMouseClicked="#onMouseClickShowTable" text="Populate table" />
       <Text layoutX="241.0" layoutY="41.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter data in the table" />
   </children>
</AnchorPane>
