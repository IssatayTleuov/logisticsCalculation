      <Label layoutX="112.0" layoutY="22.0" prefHeight="56.0" prefWidth="377.0" text="Enter the name of the new company">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <TextField fx:id="textFieldEnterCompany" layoutX="203.0" layoutY="107.0" prefHeight="25.0" prefWidth="195.0" promptText="Enter name of company" />
      <Button fx:id="btnAdd" layoutX="263.0" layoutY="156.0" mnemonicParsing="false" onMouseClicked="#onAddButtonClick" prefHeight="25.0" prefWidth="75.0" text="Add" />
      <Button fx:id="btnBack" layoutX="263.0" layoutY="203.0" mnemonicParsing="false" onMouseClicked="#onBackButtonClick" prefHeight="25.0" prefWidth="75.0" text="Back" />
   </children>
</AnchorPane>
